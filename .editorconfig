# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
charset = utf-8

# Indentation style
[*.{html,js,css,less,json,xml,java}]
insert_final_newline = true
indent_style = space
indent_size = 4
trim_trailing_whitespace = true

[*.{html,xml}]
insert_final_newline = false